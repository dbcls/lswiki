   &lt;rdfs:label rdf:datatype="<a href="http://www.w3.org/2001/XMLSchema#string" class="external free" rel="nofollow">http://www.w3.org/2001/XMLSchema#string</a>"
   &gt;Inorganic ion transport and metabolism&lt;/rdfs:label&gt;
   &lt;rdfs:subClassOf&gt;
     &lt;owl:Class rdf:about="<a href="http://purl.jp/11/cog/COGFunction" class="external free" rel="nofollow">http://purl.jp/11/cog/COGFunction</a>"/&gt;
   &lt;/rdfs:subClassOf&gt;
 &lt;/owl:Class&gt;
 &lt;owl:Class rdf:about="<a href="http://purl.jp/11/cog/L" class="external free" rel="nofollow">http://purl.jp/11/cog/L</a>"&gt;
   &lt;rdfs:label rdf:datatype="<a href="http://www.w3.org/2001/XMLSchema#string" class="external free" rel="nofollow">http://www.w3.org/2001/XMLSchema#string</a>"
   &gt;Replication, recombination and repair&lt;/rdfs:label&gt;
   &lt;rdfs:subClassOf&gt;
     &lt;owl:Class rdf:about="<a href="http://purl.jp/11/cog/COGFunction" class="external free" rel="nofollow">http://purl.jp/11/cog/COGFunction</a>"/&gt;
   &lt;/rdfs:subClassOf&gt;
 &lt;owl:Class rdf:about="<a href="http://purl.jp/11/cog/-" class="external free" rel="nofollow">http://purl.jp/11/cog/-</a>"&gt;
   &lt;rdfs:subClassOf&gt;
     &lt;owl:Class rdf:about="<a href="http://purl.jp/11/cog/COGFunction" class="external free" rel="nofollow">http://purl.jp/11/cog/COGFunction</a>"/&gt;
   &lt;/rdfs:subClassOf&gt;
   &lt;rdfs:label rdf:datatype="<a href="http://www.w3.org/2001/XMLSchema#string" class="external free" rel="nofollow">http://www.w3.org/2001/XMLSchema#string</a>"
   &gt;Not in COGs&lt;/rdfs:label&gt;
 &lt;/owl:Class&gt;
