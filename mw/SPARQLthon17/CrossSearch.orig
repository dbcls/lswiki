BiologicalDatabase_url=http://genome.microbedb.jp/cyanobase
BiologicalDatabase_name=CyanoBase
BiologicalDatabaseEntry_entryID=Synechocystis
BiologicalDatabaseEntry_taxon_BiologicalDatabaseEntry_name=Synechocystis sp. PCC 6803
BiologicalDatabaseEntry_taxon_BiologicalDatabaseEntry_url=http://www.ncbi.nlm.nih.gov/Taxonomy/Browser/wwwtax.cgi?id=1148
BiologicalDatabaseEntry_taxon_BiologicalDatabaseEntry_entryID=1148
BiologicalDatabaseEntry_reference= pmid:23110816 
</pre>
