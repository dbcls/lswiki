    SELECT REPLACE(str(?gold),&quot;http://www.genomesonline.org/cgi-bin/GOLD/GOLDCards.cgi\\?goldstamp=&quot;, &quot;GOLD:&quot; ) as&nbsp;?label&nbsp;?gold as&nbsp;?link
    FROM &lt;http://togogenome.org/gold/&gt;
