      values&nbsp;?so { obo:SO_0000340 obo:SO_0000155 }
     &nbsp;?seq rdfs:seeAlso&nbsp;?tax_id .
     &nbsp;?seq a&nbsp;?so .
     &nbsp;?seq rdfs:seeAlso&nbsp;?xref .
     &nbsp;?xref rdfs:label&nbsp;?label .
